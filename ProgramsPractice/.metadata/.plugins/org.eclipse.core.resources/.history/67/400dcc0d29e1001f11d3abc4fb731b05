package interviews.practice;

public class WordsNumber {
	public static void main(String[]args) {
		String sentence = "           Hello ho   w are you     ";
		String newSentence = sentence.strip();
		char a[]=newSentence.toCharArray();
		int count =0;

		for(int i=0;i<a.length;i++) {
			if(a[i]=='  ') {
				count++;
			}
		}
	}

}
